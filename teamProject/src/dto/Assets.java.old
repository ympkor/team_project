package dto;

public class Assets {

	private int assetsId;
	private String assetsCategory;
	private String assetsName;
	
	public Assets() {
	}
	
	public Assets(int assetsId, String assetsCategory, String assetsName) {
		super();
		this.assetsId = assetsId;
		this.assetsCategory = assetsCategory;
		this.assetsName = assetsName;
	}

	public int getAssetsId() {
		return assetsId;
	}

	public void setAssetsId(int assetsId) {
		this.assetsId = assetsId;
	}

	public String getAssetsCategory() {
		return assetsCategory;
	}

	public void setAssetsCategory(String assetsCategory) {
		this.assetsCategory = assetsCategory;
	}

	public String getAssetsName() {
		return assetsName;
	}

	public void setAssetsName(String assetsName) {
		this.assetsName = assetsName;
	}

	@Override
	public String toString() {
		return "Assets [assetsId=" + assetsId + ", assetsCategory=" + assetsCategory + ", assetsName=" + assetsName
				+ "]";
	}
}
